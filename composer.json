{
    "name": "luan-tavares/hubspot-request-body",
    "license": "MIT",
    "description": "Hubspot Request Body Constructor",
    "keywords": [
        "hubspot",
        "api",
        "integration"
    ],
    "type": "library",
    "authors": [
        {
            "name": "Luan Tavares Louren√ßo",
            "email": "luan.tavares.lourenco@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "LTL\\HubspotRequestBody\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LTL\\HubspotRequestBody\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/body"
    ],
    "require": {
        "php": "^8.2",
        "nesbot/carbon": "^3.0"
    },
    "require-dev": {
        "luan-tavares/simple-deploy": "^1.0",
        "larapack/dd": "1.*",
        "phpunit/phpunit": "^10"
    },
    "scripts": {
        "deploy-tag": [
            "LTL\\Deploy\\DeployTag::execute"
        ],
        "deploy": [
            "LTL\\Deploy\\Deploy::execute"
        ]
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}